class Solution:
    def plusOne(self, digits: List[int]) -> List[int]:
        s=''.join(map(str,digits))
        p=[]
        print(s)
        x=int(s)+1
        print(x)
        k=str(x)
        for i in k:
            p.append(int(i))
        return p
            
        
